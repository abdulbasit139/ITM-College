
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Admin Dashboard</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="@Url.Content("~/vendors/ti-icons/css/themify-icons.css")">
    <link rel="stylesheet" href="@Url.Content("~/vendors/base/vendor.bundle.base.css")">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="@Url.Content("~/css/style.css")">
    <!-- endinject -->
    <link rel="shortcut icon" href="@Url.Content("~/images/favicon.png")" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" />
    <style>
        .card a {
            text-decoration: none;
            color: black;
        }
        ul li a {
            text-decoration: none;
            color: black;
        }
    </style>
</head>
<body> 
 
    <div class="container-scroller">
    <!-- partial:partials/_navbar.html -->
    <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
      <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
        <h4 style="margin-bottom: 0px;">
            @ViewBag.adminName
        </h4>

      </div>
      <div class="navbar-menu-wrapper d-flex align-items-center justify-content-end">
        <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
          <span class="ti-view-list"></span>
        </button>
        
        <ul class="navbar-nav navbar-nav-right">
          <li class="nav-item dropdown me-1">
            <a type="button" href="@Url.Action("Logout", "Admin")" class="btn btn-dark">LOGOUT</a>
          </li>
        </ul>
        <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
          <span class="ti-view-list"></span>
        </button>
      </div>
    </nav>
    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_sidebar.html -->
      <nav class="sidebar sidebar-offcanvas" id="sidebar">
        <ul class="nav">
          <li class="nav-item">
            <a class="nav-link" href="@Url.Action("Dashboard")">
              <i class="ti-shield menu-icon"></i>
              <span class="menu-title">Dashboard</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="orders.html">
              <i class="fa fa-user menu-icon"></i>
              <span class="menu-title">Students</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="@Url.Action("Registrations", "Admin")">
              <i class="fa fa-file-lines menu-icon"></i>
              <span class="menu-title">Registrations</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="events.html">
              <i class="fa fa-chalkboard-user menu-icon"></i>
              <span class="menu-title">Teachers</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="matches.html">
              <i class="fa fa-book menu-icon"></i>
              <span class="menu-title">Courses</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="@Url.Action("AdminProfile", "Admin")">
              <i class="fa fa-circle-user menu-icon"></i>
              <span class="menu-title">Profile</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="players.html">
              <i class="fa fa-users menu-icon"></i>
              <span class="menu-title">Admins</span>
            </a>
          </li>
        </ul>
      </nav>
      <!-- partial -->
      <div class="main-panel">
        <div class="content-wrapper">
          <div class="row">
            @RenderBody()
          </div>
        </div>
        <!-- content-wrapper ends -->
        <!-- partial:partials/_footer.html -->
        <footer class="footer">
          <div class="d-sm-flex justify-content-center justify-content-sm-between">
            <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © <a href="https://www.bootstrapdash.com/" target="_blank">bootstrapdash.com </a>2021</span>
            <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Only the best <a href="https://www.bootstrapdash.com/" target="_blank"> Bootstrap dashboard </a> templates</span>
          </div>
        </footer>
        <!-- partial -->
      </div>
      <!-- main-panel ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>


  <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="@Url.Content("~/vendors/base/vendor.bundle.base.js")"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <script src="@Url.Content("~/vendors/chart.js/Chart.min.js")"></script>
    <script src="@Url.Content("~/js/jquery.cookie.js")" type="text/javascript"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="@Url.Content("~/js/off-canvas.js")"></script>
    <script src="@Url.Content("~/js/hoverable-collapse.js")"></script>
    <script src="@Url.Content("~/js/template.js")"></script>
    <script src="@Url.Content("~/js/todolist.js")"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="@Url.Content("~/js/dashboard.js")"></script>
    <!-- End custom js for this page-->
</body>

</html>